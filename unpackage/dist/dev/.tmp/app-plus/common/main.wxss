.grace-footer .grace-items{width:auto; line-height:90rpx; -webkit-flex-shrink:0; -ms-flex-negative:0; flex-shrink:0;}
.grace-footer ._button{width:100%; border:0; border-radius:0; height:90rpx; line-height:90rpx;}
