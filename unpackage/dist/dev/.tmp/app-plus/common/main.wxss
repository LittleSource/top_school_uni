.grace-list .items .title{width:100%; margin-left:18px; padding:18px 30px 18px 0; font-size:16px; height:20px; line-height:20px; overflow:hidden; border-bottom:1px solid #E9E9E9; white-space:nowrap; -o-text-overflow:ellipsis; text-overflow:ellipsis;}
