
@-webkit-keyframes grace-rotate360{0%{-webkit-transform:rotate(0deg);transform:rotate(0deg);}
50%{-webkit-transform:rotate(180deg);transform:rotate(180deg);}
100%{-webkit-transform:rotate(360deg);transform:rotate(360deg);}}
@keyframes grace-rotate360{0%{-webkit-transform:rotate(0deg);transform:rotate(0deg);}
50%{-webkit-transform:rotate(180deg);transform:rotate(180deg);}
100%{-webkit-transform:rotate(360deg);transform:rotate(360deg);}}
.grace-loading{display:-webkit-box;display:-webkit-flex;display:-ms-flexbox;display:flex; width:100%; -webkit-box-pack:center; -webkit-justify-content:center; -ms-flex-pack:center; justify-content:center; padding:16rpx 0; padding-bottom:36rpx; line-height:40rpx; color:#888;}
.grace-loading text{margin-left:12rpx;}
.grace-loading-icon{width:40rpx; height:40rpx; -webkit-box-pack:center; -webkit-justify-content:center; -ms-flex-pack:center; justify-content:center; line-height:40rpx; font-size:30rpx; text-align:center; font-family:"grace-iconfont" !important; -webkit-animation:grace-rotate360 1200ms infinite linear; animation:grace-rotate360 1200ms infinite linear;}
.grace-loading-icon:before {content:"\E9DB"; color:#888;}


@font-face {font-family: "gafont";
  src:url('data:application/x-font-woff;charset=utf-8;base64,');
}
.gaicon {
  font-family:"gafont" !important;
  font-size:22px;
  font-style:normal;
}
@-webkit-keyframes g-show{from {top:-50px;}
to {top:0;}}
@keyframes g-show{from {top:-50px;}
to {top:0;}}
.ganimate-show{-webkit-animation: g-show 300ms linear forwards;animation: g-show 300ms linear forwards;}
.icon-error:before { content: "\E604";}
.icon-right:before { content: "\E60F";}
.icon-msg:before { content: "\E623";}
.grace-alert{display:-webkit-box;display:-webkit-flex;display:-ms-flexbox;display:flex; width:90%; padding:10px 5%; -webkit-flex-wrap:nowrap; -ms-flex-wrap:nowrap; flex-wrap:nowrap; background:#FFF; position:fixed; z-index:99999; left:0; top:-60px; -webkit-box-shadow:5px 3px 3px #F7F8F9; box-shadow:5px 3px 3px #F7F8F9;}
.grace-alert .right{color:#00B26A !important;}
.grace-alert .error{color:#FF0000 !important;}
.grace-alert .msg{color:#00B26A !important;}
.grace-alert .icon{width:25px; line-height:30px; -webkit-box-pack:center; -webkit-justify-content:center; -ms-flex-pack:center; justify-content:center; -webkit-flex-shrink:0; -ms-flex-negative:0; flex-shrink:0; margin-right:12px;}
.grace-alert-msg{line-height:30px; font-size:16px; overflow:hidden; -webkit-box-pack:start; -webkit-justify-content:flex-start; -ms-flex-pack:start; justify-content:flex-start; width:100%; white-space:nowrap; -o-text-overflow:ellipsis; text-overflow:ellipsis;}


